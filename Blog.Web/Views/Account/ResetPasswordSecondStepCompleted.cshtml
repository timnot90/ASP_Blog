@using Blog.Web.Models.Shared
@{
    ViewBag.Breadcrumbs = new List<BreadcrumbModel>(new[] { new BreadcrumbModel("My Profile", "EditProfile", "Account"), new BreadcrumbModel("Reset Password", "ResetPasswordFirstStep", "Account") });
    ViewBag.Title = "Password Reset Completed";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-success">
            <div class="panel-heading">Success</div>
            <div class="panel-body">
                Your password-reset was successful. @Html.ActionLink("Login", "Login", "Account", new { area = "" }, null)
            </div>
        </div>
        <div>
        </div>
    </div>
</div>